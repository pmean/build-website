	urldate = {2011-11-21},
	date = {2009},
	pmid = {19519891},
	tags = {Follow-Up Studies, Humans, Patient Dropouts, Peer Review, Randomized Controlled Trials as Topic, Sample Size, Treatment Outcome}
}

