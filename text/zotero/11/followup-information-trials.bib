@article{akl_lost_2009,
	title = {{LOST} to follow-up Information in Trials ({LOST}-{IT}): a protocol on the potential impact},
	volume = {10},
	issn = {1745-6215},
	url = {http://www.trialsjournal.com/content/10/1/40},
